# Git aliases
alias 'gc=git commit'
alias 'gcm=git commit -m'
alias 'gaA=git add -A'
alias 'gap=git add -p'
alias 'cop=git checkout -p'
alias 'gacm=gaA; gcm'
alias 'gstat=git status'
alias 'gdiffc=git diff --cached'
alias 'gdiff=git diff'
alias 'glg=git log --graph --pretty=oneline --abbrev-commit'


